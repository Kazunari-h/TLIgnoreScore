{
    "name": "TLIgnoreScore",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "飲食店レビューサイトのスコアを非表示にする。",
    "content_scripts": [{
        "matches": [
            "https://tabelog.com/*"
        ],
        "js": [
            "rate.js"
        ],
        "run_at": "document_idle"
    }]
}